{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["ES2017", "ES2015.iterable", "ES6", "DOM"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "types": ["@figma/plugin-typings"]
  },
  "include": [
    "code.ts"
  ]
}